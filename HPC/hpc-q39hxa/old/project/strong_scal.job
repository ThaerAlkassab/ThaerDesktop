#!/bin/bash
#SBATCH --job-name=omp_strong
#SBATCH -n 1
#SBATCH -c 12
#SBATCH -o %x_%j.out
#SBATCH -e %x_%j.err

N=100000000

echo -en serial'\t'
./gauss_serial $N | awk '{ORS="\t";print $2}'
echo

for p in 1 2 4 8 12
do
        echo -en $p'\t'
        OMP_NUM_THREADS=$p ./gauss_parallel $N | awk '{ORS="\t";print $2}'
        echo
done